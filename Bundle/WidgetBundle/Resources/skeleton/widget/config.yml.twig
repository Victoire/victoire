victoire_core:
    widgets:
        {{ widget }}:
            class: "Victoire\\Widget\\{{ widget }}Bundle\\Entity\\Widget{{ widget }}"
            name: {{ widget }}
            {% if parent %}parent: {{parent}}{% endif %}

jms_translation:
    configs:
        victoire_{{ widget|lower }}_bundle:
            dirs: ["@Victoire{{ widget }}Bundle"]
            output_dir: "@Victoire{{ widget }}Bundle/Resources/translations"

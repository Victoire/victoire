        "doctrine/orm"                        : "~2.2,>=2.2.3",
        "friendsofsymfony/jsrouting-bundle"   : "2.0.*@dev",
        "friendsofsymfony/user-bundle"        : "2.0.*@dev",
        "friendsofvictoire/text-widget"       : "1.3.x-dev",
        "friendsofvictoire/button-widget"     : "1.3.x-dev",
        "incenteev/composer-parameter-handler": "~2.0",
        "infinite-networks/form-bundle"       : "dev-master",
        "jms/di-extra-bundle"                 : "1.4.*@dev",
        "jms/serializer-bundle"               : "^1.0",
        "jms/translation-bundle"              : "1.1.*@dev",
        "knplabs/gaufrette"                   : "v0.1.7",
        "knplabs/knp-menu"                    : "2.1.*@dev",
        "knplabs/knp-menu-bundle"             : "2.1.*@dev",
        "liip/imagine-bundle"                 : "1.0.*@dev",
        "nelmio/alice"                        : "1.*@dev",
        "sensio/distribution-bundle"          : ">=2.3.x-dev",
        "sensio/framework-extra-bundle"       : "~3.0",
        "sensio/generator-bundle"             : "~2.3",
        "stof/doctrine-extensions-bundle"     : "1.2.*@dev",
        "symfony/assetic-bundle"              : "~2.3",
        "symfony/monolog-bundle"              : "~2.4",
        "symfony/symfony"                     : ">=2.4.2, <=2.7.x-dev",
        "symfony/swiftmailer-bundle"          : "~2.3",
        "twig/extensions"                     : "~1.0",
        "twig/twig"                           : "~1.0",
        "snc/redis-bundle"                    : "1.1.x-dev",
        "predis/predis"                       : "^1.1@dev"
    },
    "require-dev"      : {
        "behat/behat"                 : "~3.0",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-extension"        : "~2.0",
        "behat/mink-goutte-driver"    : "~1.1",
        "behat/mink-selenium2-driver" : "~1.1",
        "behat/symfony2-extension"    : "~2.0",
        "knplabs/friendly-contexts"   : "dev-master",
        "phpunit/php-code-coverage"   : "2.2.3",
        "phpunit/phpunit-mock-objects": "2.3.7",
        "phpunit/phpunit"             : "4.6.*",
        "symfony/var-dumper"          : "~3.0@dev"
    },
    "suggest"          : {
        "friendsofvictoire/alertbutton-widget"        : "alertbutton widget",

        "friendsofvictoire/faq-widget"                : "faq widget",
        "friendsofvictoire/file-widget"               : "file widget",
        "friendsofvictoire/filter-widget"             : "filter widget",
        "friendsofvictoire/form-widget"               : "form widget",
        "friendsofvictoire/gmap-widget"               : "gmap widget",
        "friendsofvictoire/html-widget"               : "html widget",
        "friendsofvictoire/image-widget"              : "image widget",
        "friendsofvictoire/layout-widget"             : "layout widget",
        "friendsofvictoire/listing-widget"            : "listing widget",
        "friendsofvictoire/localeswitcher-widget"     : "localeswitcher widget",
        "friendsofvictoire/login-widget"              : "login widget",
        "friendsofvictoire/mailchimpnewsletter-widget": "mailchimpnewsletter widget",
        "friendsofvictoire/markdown-widget"           : "markdown widget",
        "friendsofvictoire/menu-widget"               : "menu widget",
        "friendsofvictoire/progress-widget"           : "progress widget",
        "friendsofvictoire/render-widget"             : "render widget",
        "friendsofvictoire/richlist-widget"           : "richlist widget",
        "friendsofvictoire/search-widget"             : "elastic search widget",
        "friendsofvictoire/sharethis-widget"          : "sharethis widget",
        "friendsofvictoire/siblingsmenu-widget"       : "siblingsmenu widget",
        "friendsofvictoire/simplecontactform-widget"  : "simplecontactform widget",
        "friendsofvictoire/sitemap-widget"            : "sitemap widget",
        "friendsofvictoire/slider-widget"             : "slider widget",
        "friendsofvictoire/slidernav-widget"          : "slidernav widget",
        "friendsofvictoire/socialbuttons-widget"      : "socialbuttons widget",
        "friendsofvictoire/socialpage-widget"         : "socialpage widget",
        "friendsofvictoire/statistic-widget"          : "statistic widget",
        "friendsofvictoire/tab-widget"                : "tab widget",
        "friendsofvictoire/table-widget"              : "table widget",
        "friendsofvictoire/text-widget"               : "text widget",
        "friendsofvictoire/title-widget"              : "title widget"
    },
    "autoload"         : {
        "psr-4": {
            "Acme\\AppBundle\\"          : "Tests/Functionnal/src/Acme/AppBundle",
            "Victoire\\Bundle\\"         : "Bundle",
            "Victoire\\Tests\\Features\\": "Tests/Features",
            "Victoire\\Widget\\"         : "Tests/Functionnal/src/Victoire/Widget"
        }
    },
    "minimum-stability": "dev",
    "repositories"     : [
        {
            "type": "vcs",
            "url" : "https://github.com/lenybernard/FriendlyContexts.git"
        },
        {
            "type": "vcs",
            "url" : "https://github.com/lenybernard/mink"
        },
        {
            "type": "vcs",
            "url" : "https://github.com/Charlie-Lucas/InfiniteFormBundle"
        }
    ],
    "extra"            : {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}

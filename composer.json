{
    "name": "victoire/victoire",
    "type": "symfony-bundle",
    "description": "Victoire is a Dynamic-CMS based on Symfony framework",
    "keywords": [
        "dcms",
        "victoire",
        "webstation",
        "Dynamic-CMS"
    ],
    "license": "MIT",
    "homepage": "http://victoire.io",
    "authors": [
        {
            "name": "Paul Andrieux",
            "email": "paul@victoire.io",
            "homepage": "http://github.com/paulandrieux",
            "role": "developer"
        },
        {
            "name": "Leny Bernard",
            "email": "leny@victoire.io",
            "homepage": "http://github.com/lenybernard",
            "role": "developer"
        },
        {
            "name": "AppVentus",
            "homepage": "http://appventus.com"
        },
        {
            "name": "Community Contributors",
            "homepage": "http://github.com/Victoire/Victoire/graphs/contributors"
        }
    ],
    "support": {
            "email": "contact@victoire.io"
        },
    "require": {
        "php" : ">=5.3.3",
        "appventus/alertify-bundle": "dev-master",
        "appventus/assetic-injector-bundle" : "dev-master",
        "doctrine/data-fixtures" : "dev-master",
        "doctrine/doctrine-bundle" : ">=1.2",
        "doctrine/doctrine-fixtures-bundle" : "dev-master",
        "doctrine/orm" : "~2.2,>=2.2.3",
        "friendsofsymfony/jsrouting-bundle" : "2.0.*@dev",
        "friendsofsymfony/user-bundle" : "2.0.*@dev",
        "incenteev/composer-parameter-handler" : "~2.0",
        "jms/di-extra-bundle" : "1.4.*@dev",
        "jms/translation-bundle" : "1.1.*@dev",
        "knplabs/gaufrette" : "v0.1.7",
        "knplabs/knp-menu" : "2.0.*@dev",
        "knplabs/knp-menu-bundle" : "2.0.*@dev",
        "liip/imagine-bundle" : "1.0.*@dev",
        "nelmio/alice" : "1.5.2",
        "sensio/distribution-bundle" : ">2.3",
        "sensio/framework-extra-bundle" : "~3.0",
        "sensio/generator-bundle" : "~2.3",
        "stof/doctrine-extensions-bundle" : "1.2.*@dev",
        "symfony/assetic-bundle" : "~2.3",
        "symfony/monolog-bundle" : "~2.4",
        "symfony/symfony" : ">=2.4.2, <2.7.0",
        "symfony/swiftmailer-bundle" : "~2.3",
        "twig/extensions" : "~1.0"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "behat/mink-browserkit-driver"  : "*",
        "behat/mink-extension": "~2.0",
        "behat/mink-goutte-driver": "~1.1",
        "behat/mink-selenium2-driver": "~1.1",
        "behat/symfony2-extension": "~2.0",
        "knplabs/friendly-contexts": "dev-master",
        "phpunit/phpunit": "4.6.*",
        "symfony/var-dumper": "~3.0@dev"
    },
    "suggest": {
        "friendsofvictoire/alertbutton-widget": "victoire's widget",
        "friendsofvictoire/archive-widget": "victoire's widget",
        "friendsofvictoire/articlelist-widget": "victoire's widget",
        "friendsofvictoire/breadcrumb-widget": "victoire's widget",
        "friendsofvictoire/button-widget": "victoire's widget",
        "friendsofvictoire/calculator-widget": "victoire's widget",
        "friendsofvictoire/ckeditor-widget": "victoire's widget",
        "friendsofvictoire/cover-widget": "victoire's widget",
        "friendsofvictoire/disqus-widget": "victoire's widget",
        "friendsofvictoire/faq-widget": "victoire's widget",
        "friendsofvictoire/file-widget": "victoire's widget",
        "friendsofvictoire/filter-widget": "victoire's widget",
        "friendsofvictoire/form-widget": "victoire's widget",
        "friendsofvictoire/html-widget": "victoire's widget",
        "friendsofvictoire/image-widget": "victoire's widget",
        "friendsofvictoire/layout-widget": "victoire's widget",
        "friendsofvictoire/listing-widget": "victoire's widget",
        "friendsofvictoire/login-widget": "victoire's widget",
        "friendsofvictoire/markdown-widget": "victoire's widget",
        "friendsofvictoire/menu-widget": "victoire's widget",
        "friendsofvictoire/render-widget": "victoire's widget",
        "friendsofvictoire/richlist-widget": "victoire's widget",
        "friendsofvictoire/sharethis-widget": "victoire's widget",
        "friendsofvictoire/siblingsmenu-widget": "victoire's widget",
        "friendsofvictoire/simplecontactform-widget": "victoire's widget",
        "friendsofvictoire/sitemap-widget": "victoire's widget",
        "friendsofvictoire/slider-widget": "victoire's widget",
        "friendsofvictoire/slidernav-widget": "victoire's widget",
        "friendsofvictoire/socialbuttons-widget": "victoire's widget",
        "friendsofvictoire/statistic-widget": "victoire's widget",
        "friendsofvictoire/tab-widget": "victoire's widget",
        "friendsofvictoire/table-widget": "victoire's widget",
        "friendsofvictoire/text-widget": "victoire's widget",
        "friendsofvictoire/title-widget": "victoire's widget"
    },
    "autoload": {
        "psr-4": {
            "Acme\\AppBundle\\": "Tests/Functionnal/src/Acme/AppBundle",
            "Victoire\\Bundle\\": "Bundle",
            "Victoire\\Tests\\Features\\": "Tests/Features",
            "Victoire\\Widget\\": "Tests/Functionnal/src/Victoire/Widget"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lenybernard/FriendlyContexts.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lenybernard/mink"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}

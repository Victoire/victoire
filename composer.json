{
    "name"             : "victoire/victoire",
    "type"             : "symfony-bundle",
    "description"      : "Victoire is a Dynamic-CMS based on Symfony framework",
    "keywords"         : ["DCMS", "Victoire", "FriendsofVictoire", "FoV", "Dynamic", "CMS", "WebStation", "Symfony", "Overlay"],
    "license"          : "MIT",
    "homepage"         : "http://victoire.io",
    "authors"          : [
        {
            "name"    : "Paul Andrieux",
            "email"   : "paul@victoire.io",
            "homepage": "http://github.com/paulandrieux",
            "role"    : "developer"
        },
        {
            "name"    : "Leny Bernard",
            "email"   : "leny@victoire.io",
            "homepage": "http://github.com/lenybernard",
            "role"    : "developer"
        },
        {
            "name"    : "AppVentus",
            "homepage": "http://appventus.com"
        },
        {
            "name"    : "Community Contributors",
            "homepage": "http://github.com/Victoire/Victoire/graphs/contributors"
        }
    ],
    "support"          : {
        "email": "contact@victoire.io"
    },
    "require"          : {
        "php"                                 : ">=5.3.3",
        "appventus/alertify-bundle"           : "dev-master",
        "appventus/assetic-injector-bundle"   : "dev-master",
        "doctrine/data-fixtures"              : "dev-master",
        "doctrine/doctrine-bundle"            : ">=1.2",
        "doctrine/doctrine-fixtures-bundle"   : "dev-master",
        "doctrine/orm"                        : "~2.2,>=2.2.3",
        "friendsofsymfony/jsrouting-bundle"   : "2.0.*@dev",
        "friendsofsymfony/user-bundle"        : "2.0.*@dev",
        "friendsofvictoire/text-widget"       : "dev-master",
        "friendsofvictoire/button-widget"     : "dev-master",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/di-extra-bundle"                 : "1.4.*@dev",
        "jms/serializer-bundle"               : "^1.0",
        "jms/translation-bundle"              : "1.1.*@dev",
        "knplabs/gaufrette"                   : "v0.1.7",
        "knplabs/knp-menu"                    : "2.0.*@dev",
        "knplabs/knp-menu-bundle"             : "2.0.*@dev",
        "liip/imagine-bundle"                 : "1.0.*@dev",
        "nelmio/alice"                        : "1.*@dev",
        "sensio/distribution-bundle"          : ">2.3",
        "sensio/framework-extra-bundle"       : "~3.0",
        "sensio/generator-bundle"             : "~2.3",
        "stof/doctrine-extensions-bundle"     : "1.2.*@dev",
        "symfony/assetic-bundle"              : "~2.3",
        "symfony/monolog-bundle"              : "~2.4",
        "symfony/symfony"                     : ">=2.4.2, <2.7.0",
        "symfony/swiftmailer-bundle"          : "~2.3",
        "twig/extensions"                     : "~1.0",
        "twig/twig"                           : "~1.0"
    },
    "require-dev"      : {
        "behat/behat"                 : "~3.0",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-extension"        : "~2.0",
        "behat/mink-goutte-driver"    : "~1.1",
        "behat/mink-selenium2-driver" : "~1.1",
        "behat/symfony2-extension"    : "~2.0",
        "knplabs/friendly-contexts"   : "dev-master",
        "phpunit/php-code-coverage"   : "2.2.3",
        "phpunit/phpunit-mock-objects": "2.3.7",
        "phpunit/phpunit"             : "4.6.*",
        "symfony/var-dumper"          : "~3.0@dev"
    },
    "suggest"          : {
        "friendsofvictoire/alertbutton-widget"      : "alertbutton widget",
        "friendsofvictoire/archive-widget"          : "archive widget",
        "friendsofvictoire/articlelist-widget"      : "articlelist widget",
        "friendsofvictoire/breadcrumb-widget"       : "breadcrumb widget",
        "friendsofvictoire/button-widget"           : "button widget",
        "friendsofvictoire/calculator-widget"       : "calculator widget",
        "friendsofvictoire/ckeditor-widget"         : "ckeditor widget",
        "friendsofvictoire/cover-widget"            : "cover widget",
        "friendsofvictoire/disqus-widget"           : "disqus widget",
        "friendsofvictoire/faq-widget"              : "faq widget",
        "friendsofvictoire/file-widget"             : "file widget",
        "friendsofvictoire/filter-widget"           : "filter widget",
        "friendsofvictoire/form-widget"             : "form widget",
        "friendsofvictoire/html-widget"             : "html widget",
        "friendsofvictoire/image-widget"            : "image widget",
        "friendsofvictoire/layout-widget"           : "layout widget",
        "friendsofvictoire/listing-widget"          : "listing widget",
        "friendsofvictoire/login-widget"            : "login widget",
        "friendsofvictoire/markdown-widget"         : "markdown widget",
        "friendsofvictoire/menu-widget"             : "menu widget",
        "friendsofvictoire/render-widget"           : "render widget",
        "friendsofvictoire/richlist-widget"         : "richlist widget",
        "friendsofvictoire/sharethis-widget"        : "sharethis widget",
        "friendsofvictoire/search-widget"           : "elastic search widget",
        "friendsofvictoire/siblingsmenu-widget"     : "siblingsmenu widget",
        "friendsofvictoire/simplecontactform-widget": "simplecontactform widget",
        "friendsofvictoire/sitemap-widget"          : "sitemap widget",
        "friendsofvictoire/slider-widget"           : "slider widget",
        "friendsofvictoire/slidernav-widget"        : "slidernav widget",
        "friendsofvictoire/socialbuttons-widget"    : "socialbuttons widget",
        "friendsofvictoire/statistic-widget"        : "statistic widget",
        "friendsofvictoire/tab-widget"              : "tab widget",
        "friendsofvictoire/table-widget"            : "table widget",
        "friendsofvictoire/text-widget"             : "text widget",
        "friendsofvictoire/title-widget"            : "title widget"
    },
    "autoload"         : {
        "psr-4": {
            "Acme\\AppBundle\\"          : "Tests/Functionnal/src/Acme/AppBundle",
            "Victoire\\Bundle\\"         : "Bundle",
            "Victoire\\Tests\\Features\\": "Tests/Features",
            "Victoire\\Widget\\"         : "Tests/Functionnal/src/Victoire/Widget"
        }
    },
    "minimum-stability": "dev",
    "repositories"     : [
        {
            "type": "vcs",
            "url" : "https://github.com/lenybernard/FriendlyContexts.git"
        },
        {
            "type": "vcs",
            "url" : "https://github.com/lenybernard/mink"
        }
    ],
    "extra"            : {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
